<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape >
            <gradient android:angle="270"
                android:startColor="#5E4B97"
                android:centerColor="#5A4597"
                android:endColor="#4A3781"/>
            <stroke android:color="#EFBB35" android:width="1dp" android:dashGap="2dp" android:dashWidth="3dp"/>
            <corners android:radius="6dp"/>

        </shape>


    </item>


    <item android:drawable="?selectableItemBackground"/>

</layer-list>